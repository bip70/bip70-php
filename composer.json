{
    "name": "bip70/bip70",
    "description": "Bitcoin payment requests (BIP70) toolkit for PHP",
    "type": "library",
    "keywords": ["bitcoin", "btc", "bip70", "payment", "request"],
    "autoload": {
        "psr-4": {
            "Bip70\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bip70\\Test\\": "test/unit"
        }
    },
    "require": {
        "php": ">=7.0",
        "sop/x509": "^0.6.0",
        "rgooding/protobuf-php": "v0.0.1",
        "composer/ca-bundle": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "phpstan/phpstan": "^0.9.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0.0",
        "squizlabs/php_codesniffer": "^3.1"
    },
    "license": "MIT"
}
